	"Resources": {
		"VPCNAT": {
			"Type": "AWS::CloudFormation::Stack",
			"Properties": {
				"TemplateURL": "https://s3.amazonaws.com/cf-dynamic-execution-server/1_VPC_EX_No_VPN.json",
				"Parameters": {
					"VPCCIDR": {
						"Ref": "1VPCCIDR"
					},
					"ESCIDR1": {
						"Ref": "2ESCIDR1"
					},
					"ESCIDR2": {
						"Ref": "2ESCIDR2"
					},
					"QualiServerCIDR": {
						"Ref": "4QualiServerCIDR"
					},
					"KeyPair": {
						"Ref": "5KeyPair"
					},
					"QualiXCIDR1": {
						"Ref": "3QualiXCIDR1"
					},
					"QualiXCIDR2": {
						"Ref": "3QualiXCIDR2"
					},
					"AZ1": {
						"Fn::Select" : [ "0", { "Fn::GetAZs" : { "Ref" : "AWS::Region" } } ]
					},
					"AZ2": {
						"Fn::Select" : [ "1", { "Fn::GetAZs" : { "Ref" : "AWS::Region" } } ]
					},
					"HA": {
						"Ref": "8HighAvailabilityMode"
					}
				}
			}
		},
		"Instances": {
			"Type": "AWS::CloudFormation::Stack",
			"Properties": {
				"Parameters": {
					"QualixSN1": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.QualixSN1"]
					},
					"QualixSN2": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.QualixSN2"]
					},
					"ESInstanceSN1": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.ESInstanceSN1"]
					},
					"ESInstanceSN2": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.ESInstanceSN2"]
					},
					"QualiServerInstanceSN": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.QualiServerInstanceSN"]
					},
					"QualiXInstanceProfile": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.QualiXInstanceProfile"]
					},
					"ESInstanceProfile": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.ESInstanceProfile"]
					},
					"QualiServerInstanceProfile": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.QualiServerInstanceProfile"]
					},
					"SG1id": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.SG1id"]
					},
					"SG2id": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.SG2id"]
					},
					"SG3id": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.SG3id"]
					},
					"QualixAMI": {
						"Fn::FindInMap": ["AMIFromRegion",
						{
							"Ref": "AWS::Region"
						},
						"QualixAMI"]
					},
					"ESAMI": {
						"Fn::FindInMap": ["AMIFromRegion",
						{
							"Ref": "AWS::Region"
						},
						"ESAMI"]
					},
					"QSAMI": {
						"Fn::FindInMap": ["AMIFromRegion",
						{
							"Ref": "AWS::Region"
						},
						"QSAMI"]
					},
					"KeyPair": {
						"Ref": "5KeyPair"
					},
					"ESInstance2Profile": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.ESInstance2Profile"]
					},
					"ZUsername": {
						"Ref": "ZUsername"
					},
					"ZPassword": {
						"Ref": "ZPassword"
					},
					"InstanceSize": {
						"Ref": "6InstanceType"
					},
					"CloudShellServerInstanceSize": {
						"Ref": "7CloudShellServerInstanceType"
					},
					"ExecutionServerBinaryLocation": "https://s3.amazonaws.com/cf-dynamic-execution-server/execution-server/cloudshell-version/exec.tar"
				},
				"TemplateURL": "https://s3.amazonaws.com/cf-dynamic-execution-server/2_EC2_EX_No_VPN.json"
			}
		}
	},
	"Outputs": {
		"ManagementVPCID": {
			"Description": "Management VPC ID",
			"Value": {
				"Fn::GetAtt": ["VPCNAT",
				"Outputs.VPCId"]
			}
		},
		"SG1id": {
			"Description": "SG1 ID",
			"Value": {
				"Fn::GetAtt": ["VPCNAT",
				"Outputs.SG1id"]
			}
		},
		"S3Name": {
			"Description": "Name of S3 bucket",
			"Value": {
				"Fn::GetAtt": ["VPCNAT",
				"Outputs.S3Name"]
			}
		},
		"QualixInstanceEIP": {
			"Description": "Qualix Instance Elastic IP",
			"Value": {
				"Fn::GetAtt": ["Instances",
				"Outputs.QualixInstanceEIP"]
			}
		}
	}
}