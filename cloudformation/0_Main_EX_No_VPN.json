	"Resources": {
		"VPCNAT": {
			"Type": "AWS::CloudFormation::Stack",
			"Properties": {
				"TemplateURL": "https://s3.amazonaws.com/cf-dynamic-execution-server/1_VPC_EX_No_VPN.json",
				"Parameters": {
					"VPCCIDR": {
						"Ref": "1VPCCIDR"
					},
					"ESCIDR": {
						"Ref": "2ESCIDR"
					},
					"QualiServerCIDR": {
						"Ref": "3QualiServerCIDR"
					},
					"OnPremiseCIDR": {
						"Ref": "4OnPremiseCIDR"
					},
					"KeyPair": {
						"Ref": "7KeyPair"
					},
					"QualiCIDR": {
						"Ref": "3QualiCIDR"
					},
					"AZ": {
						"Fn::FindInMap": ["AZFromRegion",
						{
							"Ref": "AWS::Region"
						},
						"AZ"]
					}
				}
			}
		},
		"Instances": {
			"Type": "AWS::CloudFormation::Stack",
			"Properties": {
				"Parameters": {
					"QualixSN": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.QualixSN"]
					},
					"ESInstanceSN": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.ESInstanceSN"]
					},
					"QualiServerInstanceSN": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.QualiServerInstanceSN"]
					},
					"QualiXInstanceProfile": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.QualiXInstanceProfile"]
					},
					"ESInstanceProfile": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.ESInstanceProfile"]
					},
					"QualiServerInstanceProfile": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.QualiServerInstanceProfile"]
					},
					"SG1id": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.SG1id"]
					},
					"SG2id": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.SG2id"]
					},
					"SG3id": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.SG3id"]
					},
					"QualixAMI": {
						"Fn::FindInMap": ["AMIFromRegion",
						{
							"Ref": "AWS::Region"
						},
						"QualixAMI"]
					},
					"ESAMI": {
						"Fn::FindInMap": ["AMIFromRegion",
						{
							"Ref": "AWS::Region"
						},
						"ESAMI"]
					},
					"KeyPair": {
						"Ref": "7KeyPair"
					},
					"ESInstance2Profile": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.ESInstance2Profile"]
					},
					"ZUsername": {
						"Ref": "ZUsername"
					},
					"ZPassword": {
						"Ref": "ZPassword"
					},
					"InstanceSize": {
						"Ref": "8InstanceType"
					},
					"CloudShellServerInstanceSize": {
						"Ref": "CloudShellServerInstanceSize"
					},
					"ExecutionServerBinaryLocation": "https://s3.amazonaws.com/cf-dynamic-execution-server/execution-server/cloudshell-version/exec.tar"
				},
				"TemplateURL": "https://s3.amazonaws.com/cf-dynamic-execution-server/2_EC2_EX_No_VPN.json"
			}
		}
